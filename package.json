{
  "name": "@cita/web3",
  "version": "0.1.4",
  "main": "lib/index.js",
  "repository": "https://github.com/CITA-Toys/web3",
  "author": "Keith <keithwhisper@gmail.com>",
  "license": "MIT",
  "scripts": {
    "protoc": "protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --proto_path=src/cita-proto --ts_out=./src/cita-proto --js_out=import_style=commonjs,binary:./src/cita-proto src/cita-proto/blockchain.proto",
    "start": "nodemon --config nodemon.json",
    "debug": "node debug/index.js",
    "build": "tsc && cp -r ./src/cita-proto ./lib",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.2.7",
    "@types/node": "^10.1.4",
    "@types/underscore": "^1.8.8",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-typescript": "^0.12.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.2",
    "nodemon": "^1.17.5",
    "prettier": "^1.13.3",
    "ts-node": "^6.0.5",
    "ts-protoc-gen": "^0.7.3",
    "typescript": "^2.8.4",
    "typescript-eslint-parser": "^15.0.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "crypto-js": "^3.1.9-1",
    "elliptic": "^6.4.0",
    "google-protobuf": "^3.5.0",
    "web3": "^1.0.0-beta.34"
  }
}
